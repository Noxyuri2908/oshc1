{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "browner12/helpers": "^2.1",
        "elibyy/tcpdf-laravel": "^5.8",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.7",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "orangehill/iseed": "^2.6",
        "paypal/rest-api-sdk-php": "^1.14",
        "phpmailer/phpmailer": "^6.0",
        "phpoffice/phpspreadsheet": "^1.14",
        "rap2hpoutre/fast-excel": "^2.5",
        "rap2hpoutre/laravel-log-viewer": "^1.2",
        "spatie/laravel-permission": "^3.17",
        "spipu/html2pdf": "^5.2",
        "tucker-eric/eloquentfilter": "^2.4",
        "tymon/jwt-auth": "^1.0",
        "unisharp/laravel-ckeditor": "^4.7",
        "unisharp/laravel-filemanager": "dev-master"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "kitloong/laravel-migrations-generator": "^4.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "barryvdh/laravel-debugbar": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/simple_html_dom.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Repositories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Repositories\\" : "app/Repositories/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
